    <exercise>
          <statement>
            <p>
              Find the unit vector in the first quadrant of <m>\mathbb{R}^2</m> that makes a
              <m>50^{\circ}</m> angle with the <m>x</m>-axis.
            </p>
          </statement>
          <answer>
            <p>
              <m>\la \cos(\frac{50\pi}{180}), \sin(\frac{50\pi}{180}) \ra</m>
            </p>
          </answer>
    </exercise>

    <exercise>
          <statement>
            <p>
              Find the unit vector in the second quadrant of <m>\mathbb{R}^2</m> that makes a
              <m>30^{\circ}</m> angle with the <m>y</m>-axis.
            </p>
          </statement>
          <answer>
            <p>
              <m>\la \frac{-1}{2}, \frac{\sqrt{3}}{2} \ra</m>
            </p>
          </answer>
    </exercise>

    <exercise>
          <statement>
            <p>
              Verify, from <xref ref="idea_unit_vectors">Key Idea</xref>,
              that
              <me>
                \vec u=\la \sin(\theta) \cos(\varphi) ,\sin(\theta) \sin(\varphi) ,\cos(\theta) \ra
              </me>
              is a unit vector for all angles <m>\theta</m> and <m>\varphi</m>.
            </p>
          </statement>
          <answer>
            <p>
              <md>
                <mrow>\norm{\vec u} \amp = \sqrt{\sin^2(\theta) \cos^2(\varphi) +\sin^2(\theta) \sin^2(\varphi) +\cos^2(\theta) }</mrow>
                <mrow>\amp = \sqrt{\sin^2(\theta) (\cos^2(\varphi) +\sin^2(\varphi) )+\cos^2(\theta) }</mrow>
                <mrow>\amp = \sqrt{\sin^2(\theta) +\cos^2(\theta) }</mrow>
                <mrow>\amp =1</mrow>
              </md>.
            </p>
          </answer>
    </exercise>

