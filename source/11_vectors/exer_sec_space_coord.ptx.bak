                    // setup and draw the axes
                    real[] myxchoice={};
                    real[] myychoice={-3,3};
                    real[] myzchoice={-6,6};
                    defaultpen(0.5mm);
                    pair xbounds=(-2,2);
                    pair ybounds=(-2,2);
                    pair zbounds=(-2*pi,2*pi);

                    xaxis3("",xbounds.x,xbounds.y,black,OutTicks(myxchoice),Arrow3(size=3mm));
