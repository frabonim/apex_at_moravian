                    triple g(real t) {return (t,cos(-3*pi/2),-3*pi/2);}
                    path3 mypath=graph(g,-2,2,operator ..); draw(mypath,redpen);
                    triple g(real t) {return (t,cos(-pi/2),-pi/2);}
                    path3 mypath=graph(g,-2,2,operator ..); draw(mypath,redpen);
                    triple g(real t) {return (t,cos(0),0);}
                    path3 mypath=graph(g,-2,2,operator ..); draw(mypath,redpen);
                    triple g(real t) {return (t,cos(3*pi/2),3*pi/2);}
                    path3 mypath=graph(g,-2,2,operator ..); draw(mypath,redpen);
                    triple g(real t) {return (t,cos(pi/2),pi/2);}
                    path3 mypath=graph(g,-2,2,operator ..); draw(mypath,redpen);
                    triple g(real t) {return (0,cos(t),t);}
                    path3 mypath=graph(g,-2*pi,pi*2,operator ..); draw(mypath,bluepen);

                  </asymptote>
                </image>
               <!-- figures/fig10_01_ex_16_3D.asy END -->
