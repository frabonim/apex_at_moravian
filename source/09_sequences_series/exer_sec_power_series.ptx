    <exercise>
      <statement>
        <p>
          <m>\displaystyle \sum _{n=1}^\infty \frac{x^n}{n^2}</m>
        </p>
      </statement>
      <answer>
        <ol>
          <li>
            <p>
              <m>R=1</m>
            </p>
          </li>
          <li>
            <p>
              <m>[-1,1]</m>
            </p>
          </li>
        </ol>
      </answer>
    </exercise>
    <exercise>
      <statement>
        <p>
          <m>\displaystyle \sum _{n=1}^\infty \frac{(x+2)^n}{n^3}</m>
        </p>
      </statement>
      <answer>
        <ol>
          <li>
