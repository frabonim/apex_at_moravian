      <exercise>
        <statement>
          <p>
            Plot the points with the given polar coordinates.
          </p>

          <p>
            <ol cols="2">
              <li>
                <p>
                  <m>A=P(2,0)</m>
                </p>
              </li>

              <li>
                <p>
                  <m>B=P(1,\pi)</m>
                </p>
              </li>

              <li>
                <p>
                  <m>C=P(-2,\pi/2)</m>
                </p>
              </li>

              <li>
                <p>
                  <m>D=P(1,\pi/4)</m>
                </p>
              </li>
            </ol>
          </p>
        </statement>
        <answer>
          <image xml:id="img_polar7bX1" width="47%">
            <description></description>
            <latex-image>

            \begin{tikzpicture}

            \foreach \x in {1,2}
            {
              \draw (0,0) circle (\x);
              \draw (\x,0) node [below right] {\(\x\)};
            }
            \draw [thick,-&gt;,&gt;=stealth] (0,0) node [below] {\(O\)} -- (2.5,0);

            \filldraw (xyz polar cs: angle=0,radius=2) circle (2pt) node [above right] {\(A\)}
                      (xyz polar cs: angle=180,radius=1)circle (2pt) node [right] {\(B\)}
                      (xyz polar cs: angle=90,radius=-2)circle (2pt) node [right] {\(C\)}
                      (xyz polar cs: angle=45,radius=1)circle (2pt) node [right] {\(D\)};

            \end{tikzpicture}

            </latex-image>
          </image>
        </answer>
      </exercise>

