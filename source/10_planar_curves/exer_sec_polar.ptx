      <exercise>
            <statement>
              <p>
                For each of the given points give two sets of polar coordinates that identify it,
                where <m>0\leq \theta\leq 2\pi</m>.
              </p>

                <image xml:id="img_polar7bX4" width="47%">
                  <description></description>
                  <latex-image>

                  \begin{tikzpicture}

                  \draw [dashed,gray] (-3.1,0) -- (0,0);
                  \draw [thick,-&gt;,&gt;=stealth] (0,0) node [below] {\(O\)} -- (3.5,0);

                  \filldraw (0,0) circle (1.5pt);

                  \foreach \x in {1,2,3}
                  {
                    \draw (0,0) circle (\x cm);
                    \draw (\x,0) node [below right] {\x};
                  }

                  \foreach \x in {30,45,60,90,120,135,150}
                  {
                    \draw [rotate=\x,dashed,gray] (-3.1,0) -- (3.1,0);
                  }

                  \filldraw (xyz polar cs: angle=45,radius=2.5) circle (2pt) node [above] {\(A\)}
                            (xyz polar cs: angle=-30,radius=1)circle (2pt) node [below] {\(B\)}
                            (xyz polar cs: angle=240,radius=3)circle (2pt) node [right] {\(C\)}
                            (xyz polar cs: angle=120,radius=1.5)circle (2pt) node [right] {\(D\)};

                  \end{tikzpicture}

                  </latex-image>
                </image>

            </statement>
            <answer>
              <p>
                <m>A=P(2.5,\pi/4)</m> and <m>P(-2.5,5\pi/4)</m>;
              </p>

              <p>
                <m>B=P(-1,5\pi/6)</m> and <m>P(1,11\pi/6)</m>;
              </p>

              <p>
                <m>C=P(3,4\pi/3)</m> and <m>P(-3,\pi/3)</m>;
              </p>

              <p>
                <m>D=P(1.5,2\pi/3)</m> and <m>P(-1.5,5\pi/3)</m>;
              </p>
            </answer>
      </exercise>

      <exercise>
            <statement>
              <p>
                Convert each of the following polar coordinates to rectangular,
                and each of the following rectangular coordinates to polar.
              </p>

              <p>
                <ol cols="2" marker="a">
                  <li>
                    <p>
                      <m>A=P(2,\pi/4)</m>
                    </p>

                    <p>
                      <m>(x,y)=</m>
                    </p>
                  </li>

                  <li>
                    <p>
                      <m>B=P(2,-\pi/4)</m>
                    </p>

                    <p>
                      <m>(x,y)=</m>
                    </p>
                  </li>

                  <li>
                    <p>
                      <m>C=(2,-1)</m>
                    </p>

                    <p>
                      <m>P(r,\theta)=P</m>
                    </p>
                  </li>

                  <li>
                    <p>
                      <m>D=(-2,1)</m>
                    </p>

                    <p>
                      <m>P(r,\theta)=P</m>
                    </p>
                  </li>
                </ol>
              </p>
            </statement>
            <answer>
              <p>
                <m>A=(\sqrt{2},\sqrt{2})</m>
              </p>

              <p>
                <m>B=(\sqrt{2},-\sqrt{2})</m>
              </p>

              <p>
                <m>C=P(\sqrt{5},-0.46)</m>
              </p>

              <p>
                <m>D=P(\sqrt{5},2.68)</m>
              </p>
            </answer>
      </exercise>

      <exercise>
          <statement>
            <p>
              Convert each of the following polar coordinates to rectangular,
              and each of the following rectangular coordinates to polar.
            </p>

            <p>
              <ol cols="2" marker="a">
