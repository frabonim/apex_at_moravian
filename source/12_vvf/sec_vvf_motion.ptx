                \draw [thick,-&gt;,firstcolor,&gt;=stealth] (axis cs:-.9,.81) -- (axis cs: -.89,.7921);
                \draw (axis cs: .5,.6) node { $\vec r_2(t)$};

                \end{axis}

                \end{tikzpicture}

