              pair xbounds=(-1.1,1.1);
              pair ybounds=(-1.1,1.1);
              pair zbounds=(-.1,1.1);

              xaxis3("",xbounds.x,xbounds.y,black,OutTicks(myxchoice),Arrow3(size=3mm));
